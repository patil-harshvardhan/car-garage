{
  "version": 2,
  "builds": [
    {
      "src": "vite.config.js",
      "use": "@vitejs/vercel"
    },
    {
      "src": "api/proxy.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/proxy"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
